<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="wrap_content"
	android:orientation="vertical">

	<TextView
		android:id="@+id/contract_title"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:paddingVertical="@dimen/mypage_menu_paddingV"
		android:paddingHorizontal="@dimen/mypage_menu_paddingH"
		android:fontFamily="@font/nanum_square_ac"
		android:textSize="@dimen/mypage_content_text"
		android:textColor="@color/text_color"
		android:text="이용약관"/>

	<View
		android:layout_width="match_parent"
		android:layout_height="@dimen/split_line"
		android:background="@color/split_line"
		android:layout_marginHorizontal="@dimen/history_split_paddingH"/>
</LinearLayout>